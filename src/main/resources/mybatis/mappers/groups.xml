<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="groups">
	
	
		<insert id="insert" parameterType="groupvo">
	 	<![CDATA[
			insert 
			  into groups(no, name)
			 value (#{no}, #{name});
		]]>
	</insert>
	
</mapper>
