<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="groupuser">

	<insert id="insert" parameterType="groupUservo">
	 	<![CDATA[
			insert 
			  into groupuser (u_no, g_no, a_no)
			 value (#{uNo}, #{gNo}, #{aNo});
		]]>
	</insert>

</mapper>

